<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.application.audit.module.rateconfig.dao.ConfigDAO">
    <select id="getConfig" resultType="com.application.audit.module.rateconfig.entity.ConfigBO">
        select
        id as id,
        pass_num as passNum,
        pass_rate as passRate,
        num_status as  numStatus,
        rate_status as rateStatus
        from t_config
        limit 1
    </select>
</mapper>
